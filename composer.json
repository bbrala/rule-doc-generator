{
    "name": "symplify/rule-doc-generator",
    "description": "Documentation generator for coding standard or static analysis rules",
    "license": "MIT",
    "bin": [
        "bin/rule-doc-generator"
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/console": "^6.0",
        "symfony/string": "^6.0",
        "symfony/translation-contracts": "^3.0",
        "nette/robot-loader": "^3.4",
        "symplify/symplify-kernel": "^11.1",
        "symplify/rule-doc-generator-contracts": "^11.1",
        "symplify/package-builder": "^11.1",
        "nette/utils": "^3.2",
        "sebastian/diff": "^4.0",
        "symplify/php-config-printer": "^11.1",
        "symplify/smart-file-system": "^11.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.7.15",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\Tests\\": "tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.1-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<11.0.4",
        "symplify/composer-json-manipulator": "<11.0.4",
        "symplify/easy-coding-standard": "<11.0.4",
        "symplify/phpstan-rules": "<11.0.4",
        "symplify/easy-testing": "<11.0.4",
        "symplify/autowire-array-parameter": "<11.0.4",
        "symplify/phpstan-extensions": "<11.0.4",
        "symplify/vendor-patches": "<11.0.4",
        "symplify/skipper": "<11.0.4",
        "symplify/symfony-static-dumper": "<11.0.4",
        "symplify/monorepo-builder": "<11.0.4",
        "symplify/config-transformer": "<11.0.4",
        "symplify/easy-ci": "<11.0.4",
        "symplify/coding-standard": "<11.0.4",
        "symplify/easy-parallel": "<11.0.4",
        "symplify/neon-config-dumper": "<11.0.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

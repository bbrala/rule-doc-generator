{
    "name": "symplify/rule-doc-generator",
    "description": "Documentation generator for coding standard or static analysis rules",
    "license": "MIT",
    "bin": [
        "bin/rule-doc-generator"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "nette/robot-loader": "^3.2",
        "symplify/symplify-kernel": "^8.4",
        "symplify/markdown-diff": "^8.4",
        "nette/neon": "^3.2",
        "migrify/php-config-printer": "^0.3.50"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4",
        "phpstan/phpstan": "^0.12.40",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
